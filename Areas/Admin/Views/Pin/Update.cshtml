@model rentend.Models.ViewModels.PinViewModel;
<header>
	<h1>Update pin</h1>
	<div>
		<a asp-action="Index" class="_btn">Go back</a>
	</div>
</header>
<div class="admin_create">
	<div class="create_container">
		<form method="post">
			<input type="hidden" asp-for="@Model.pin.Id">
			<div class="form_right">
				<div class="form_tile">
					<select asp-for="@Model.pin.CarId" asp-items='@Model.Cars.ToSelectListItem(1, "Model")'></select>
				</div>
				<div class="atr_buttons">
					<input type="submit" class="btn btn_green" value="Submit">
					<a class="btn btn_red">Cancel</a>
				</div>
			</div>
		</form>
	</div>
</div>